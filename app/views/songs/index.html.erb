<table>
    <tbody>
    <% @songs.each do |song| %>
    <tr>
        <td><%= link_to "Delete", song, method: :delete, data: { confirm: "you sure?" } %> <%= link_to song.title, song_path(song)%></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
    </tr>
    <% end %>
    </tbody>
</table>